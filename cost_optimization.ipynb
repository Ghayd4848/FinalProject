    "# STEP 2: SOLVE TSP\n",
    "\n",
    "def solve_tsp(cost_matrix): \n",
    "    n = len(cost_matrix)\n",
    "    manager = pywrapcp.RoutingIndexManager(n, 1, 0)\n",
    "    routing = pywrapcp.RoutingModel(manager)\n",
    "\n",
    "    def cost_callback(from_idx, to_idx): # Defines \"distance\" (cost) between any two nodes\n",
    "        from_node = manager.IndexToNode(from_idx)\n",
    "        to_node = manager.IndexToNode(to_idx)\n",
    "        return int(cost_matrix[from_node][to_node] * 100)\n",
    "\n",
    "    transit_callback_idx = routing.RegisterTransitCallback(cost_callback)\n",
    "    routing.SetArcCostEvaluatorOfAllVehicles(transit_callback_idx)\n",
    "\n",
    "    params = pywrapcp.DefaultRoutingSearchParameters()\n",
    "    params.first_solution_strategy = routing_enums_pb2.FirstSolutionStrategy.PATH_CHEAPEST_ARC # Greedily chooses the cheapest next leg\n",
    "\n",
    "    solution = routing.SolveWithParameters(params)\n",
    "    if not solution:\n",
    "        raise Exception(\"TSP solution not found\")\n",
    "\n",
    "    index = routing.Start(0)\n",
    "    route = []\n",
    "    while not routing.IsEnd(index):\n",
    "        route.append(manager.IndexToNode(index))\n",
    "        index = solution.Value(routing.NextVar(index))\n",
    "    route.append(manager.IndexToNode(index))\n",
    "    return route"
   ]
