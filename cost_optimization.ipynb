   "source": [
    "# STEP 1: BUILD COST MATRIX\n",
    "\n",
    "def get_drive_info(origin, destination):\n",
    "    try:\n",
    "        route = gmaps.directions(origin, destination, mode=\"driving\") # Response object from Google API\n",
    "        print(f\"Google Maps call: {origin} → {destination} → {bool(route)}\") # Print whether API call returned a result (T/F)\n",
    "        if route:\n",
    "            leg = route[0]['legs'][0] # Extract first route leg (since may be multiple alternative routes)\n",
    "            distance_mi = leg['distance']['value'] * 0.000621371 # Convert meters to miles\n",
    "            duration_hr = leg['duration']['value'] / 3600 # Convert seconds to hours\n",
    "            return distance_mi, duration_hr\n",
    "    except Exception as e:\n",
    "        print(f\"Drive error: {origin} → {destination} → {e}\") # Report any exception\n",
    "    return None, None # If failed or no result, return None values\n",
    "\n",
    "def get_flight_cost_amadeus(orig_airport, dest_airport, access_token):\n",
    "    headers = {\n",
    "        \"Authorization\": f\"Bearer {access_token}\"\n",
    "    }\n",
    "    params = {\n",
    "        \"originLocationCode\": orig_airport,\n",
    "        \"destinationLocationCode\": dest_airport,\n",
    "        \"departureDate\": \"2025-12-15\",\n",
    "        \"adults\": 1,\n",
    "        \"travelClass\": \"ECONOMY\",\n",
    "        \"currencyCode\": \"USD\",\n",
    "        \"max\": 1\n",
    "    }\n",
    "    response = requests.get(\n",
    "        \"https://test.api.amadeus.com/v2/shopping/flight-offers\",\n",
    "        headers=headers,\n",
    "        params=params\n",
    "    )\n",
    "    try:\n",
    "        offers = response.json().get(\"data\", [])\n",
    "        if offers:\n",
    "            return float(offers[0][\"price\"][\"total\"])\n",
    "    except:\n",
    "        pass\n",
    "    return None\n",
    "\n",
    "def build_cost_matrix(locations, nearest_airport, access_token):\n",
    "    n = len(locations) # Total number of destinations\n",
    "    cost_matrix = np.full((n, n), np.inf) # Initialize matrix of travel costs\n",
    "    mode_matrix = [['' for _ in range(n)] for _ in range(n)] # Initialize matrix to store mode of transport (drive or fly)\n",
    "\n",
    "    for i in range(n): # Iterate through every origin-destination pair\n",
    "        for j in range(n):\n",
    "            if i == j:\n",
    "                continue # Skip self-loops\n",
    "            A, B = locations[i], locations[j]\n",
    "\n",
    "            # Step 1: Try driving directly\n",
    "            dist_drive, _ = get_drive_info(A, B)\n",
    "            cost_drive = dist_drive * 0.58 if dist_drive else np.inf # Driving cost = $0.50 / mile\n",
    "\n",
    "            # If it's a short distance (<150 miles), skip flying entirely and just compute driving cost\n",
    "            if dist_drive is not None and dist_drive < 150:\n",
    "                cost_matrix[i][j] = cost_drive\n",
    "                mode_matrix[i][j] = \"drive\"\n",
    "                print(f\"{A} → {B}: drive = ${cost_drive:.2f}, fly = skipped (short distance)\")\n",
    "                continue\n",
    "\n",
    "            # Step 2: Extract flight info\n",
    "            airport_A, airport_B = nearest_airport[A], nearest_airport[B] # Identify nearest airports\n",
    "            drive1, _ = get_drive_info(A, airport_A) # Drive from A to Airport A\n",
    "            drive2, _ = get_drive_info(airport_B, B) # Drive from Airport B to B\n",
    "            flight_price = get_flight_cost_amadeus(airport_A, airport_B, access_token) # Extract flight price\n",
    "\n",
    "            if None not in [drive1, drive2, flight_price]: # If all legs are successful\n",
    "                cost_fly = flight_price + 0.5 * (drive1 + drive2) # Flight + driving costs\n",
    "            else:\n",
    "                cost_fly = np.inf # If at least one leg fails, it will return infinity\n",
    "\n",
    "            print(f\"{A} → {B}: drive = ${cost_drive:.2f}, fly = ${cost_fly:.2f}\")\n",
    "\n",
    "            # Step 3: Choose cheapest mode of transport\n",
    "            if cost_drive < cost_fly:\n",
    "                cost_matrix[i][j] = cost_drive\n",
    "                mode_matrix[i][j] = \"drive\"\n",
    "            else:\n",
    "                cost_matrix[i][j] = cost_fly\n",
    "                mode_matrix[i][j] = \"fly\"\n",
    "\n",
    "            time.sleep(1)  # API rate limit buffer\n",
    "\n",
    "    return cost_matrix, mode_matrix"
   ]
